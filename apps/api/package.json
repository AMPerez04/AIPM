{
  "name": "@ai-hackathon/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "pnpm db:generate && tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "db:generate": "pnpm exec prisma generate",
    "db:migrate": "pnpm exec prisma migrate dev",
    "db:seed": "pnpm exec prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@shared/types": "workspace:*",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "twilio": "^4.20.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "eslint": "^8.56.0",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
